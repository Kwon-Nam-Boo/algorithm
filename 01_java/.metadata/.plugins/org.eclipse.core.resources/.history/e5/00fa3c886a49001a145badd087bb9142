package com.ssafy;

public class CarMgr extends Car {
	
	private Car[] cars = new Car[100];
	private int index;
	
	public void add(Car c) {
		cars[index++] =c;
	}
	public Car[] search(){
		return cars;
	}
	public Car serach(String num) {
		for(Car c : cars) {
			if(c.getNum().equals(num)) {
				return c;
			}
		}
		return new Car();
		
	}
