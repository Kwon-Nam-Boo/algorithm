package com.ssafy;

public class MovieMgr extends Movie {
	
	
	private static MovieMgr instance;
	
	public static MovieMgr getinstance() {
		if(instance == null) {
			instance = new MovieMgr();
		}
		return instance;
	}

	public MovieMgr() {
		
	}

	public MovieMgr(String title, String director, int grade, String genre) {
		super(title, director, grade, genre);
		
	}

	public MovieMgr(String title, String director, int grade, String genre, String summary) {
		super(title, director, grade, genre, summary);
		
	}

}
