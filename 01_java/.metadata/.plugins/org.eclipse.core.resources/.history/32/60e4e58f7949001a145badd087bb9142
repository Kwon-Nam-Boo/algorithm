package com.ssafy;

public class MovieMgr extends Movie {
	
	private Movie[] movies = new Movie[100];
	private int index;
	
	private static MovieMgr instance;
	
	public static MovieMgr getinstance() {
		if(instance == null) {
			instance = new MovieMgr();
		}
		return instance;
	}
	
	public void add(Movie m) {
		movies[index++]=m;
	}
	public Movie[] search() {
		for(Movie mv : movies) {
			
		}
	}
	

}
