package com.ssafy.product;

import java.util.ArrayList;

public class ProductTest {

	public static void main(String[] args) {
		ProductMgrImpl pmgr = new ProductMgrImpl();
		
		pmgr.addProduct(new TV(1, "벽걸이", 300, 3,"LG", 100));
		pmgr.addProduct(new TV(2, "소형", 100, 1,"Samsung", 30));
		pmgr.addProduct(new Refrigerator(3, "김치", 200, 4, 300));
		pmgr.addProduct(new Refrigerator(3, "대형", 500, 2, 500));
		
		ArrayList<Product> test = pmgr.getAllProducts();
		
		for (int i = 0; i < test.size(); i++) {
			System.out.println(test.get(i));
		}

	}

}
