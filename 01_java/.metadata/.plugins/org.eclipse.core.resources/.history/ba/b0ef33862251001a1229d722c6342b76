package com.computer;

public class Monitor {
	private String maker;
	private double size;	

	public Monitor(){}
	
	public Monitor(String maker, double size) {
		super();
		this.maker = maker;
		this.size = size;
	}

	public void powerOn(){
		System.out.println("Monitor_PowerOn");	
	}

	public void powerOff(){
		System.out.println("Monitor_PowerOff");	
		
	}

	
	public void info() {
		System.out.println("------------------------");
	}
	
	
	
}


